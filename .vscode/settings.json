{
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.next": true,
        "**/.devcontainer": true,
        "**/next-env.d.ts": true,
        "**/.prettierrc": false,
        "**/.eslintrc.js": false,
        "**/babel.config.js": false,
        "**/.gitignore": true,
        "**/yarn-error.log": true,
        "**/yarn.lock": true,
        "**/node_modules": true,
        "**/tsconfig.json": false
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    }
}
